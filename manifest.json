{
    "manifest_version": 3,
    "name": "UET LMS DMC ",
    "version": "6.0",
    "description": "Calculate your DMC score for UET LMS",
 
    "content_scripts": [
      {
        "matches": ["https://lms.uet.edu.pk/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }